function factorial(numero) {
  if (numero === 0 || numero === 1) {
    return 1; // El factorial de 0 y 1 es siempre 1
  } else {
    return numero * factorial(numero - 1);
  }
}

var numero = 5; // Cambia este valor al n√∫mero del cual deseas calcular el factorial
var resultadoFactorial = factorial(numero);
console.log("El factorial de " + numero + " es " + resultadoFactorial);
